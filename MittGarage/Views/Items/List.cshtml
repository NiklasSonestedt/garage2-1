@model IEnumerable<MittGarage.Models.Vehicle>

@{
    ViewBag.Title = "Mitt Garage";
}

<h2>
    @Html.ActionLink("Mitt Garage", "Index")</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div>
    <form method="get">

        <input type="search" name="term" />
        <input type="submit" value="Hitta" />

    </form>

</div>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegNr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Owner)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.checkInDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegNr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Owner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.checkInDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Detail", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

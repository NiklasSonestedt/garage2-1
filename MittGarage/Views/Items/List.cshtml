@model IEnumerable<MittGarage.Models.Vehicle>

@{
    ViewBag.Title = "Mitt Garage";
}
<table class="lists">
    <tr>
        <th>
            <div>Regnummer</div>
            
        </th>
        <th>
            <div>Ägare</div>
           
        </th>
        <th>
            <div>Incheckad</div>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegNr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Owner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.checkInDate)
        </td>
        <td>
            @Html.ActionLink("Checka ut", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
